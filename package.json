{
    "name": "instant-website",
    "description": "Manifest a business idea as a simple webpage - using custom text and images supplied in a URL and no coding",
    "author": {
        "name": "Michael Currin",
        "url": "https://MichaelCurrin.github.io/"
    },
    "license": "MIT",
    "homepage": "https://instant-website.netlify.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/MichaelCurrin/instant-website.git"
    },
    "keywords": [
        "react",
        "react-url-query",
        "unsplash",
        "query-strings"
    ],
    "dependencies": {
        "history": "^4.2",
        "prop-types": "^15.5",
        "react": "^16.13",
        "react-dom": "^16.13",
        "react-url-query": "^1",
        "bulma": "^0.9"
    },
    "devDependencies": {
        "react-scripts": "^0.6"
    },
    "scripts": {
        "cp-deps": "cp node_modules/bulma/css/*.css public/css/",
        "start": "npm run cp-deps && react-scripts start",
        "build": "npm run cp-deps && react-scripts build",
        "test": "react-scripts test --env=jsdom"
    }
}
