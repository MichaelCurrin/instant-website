{
  "name": "instant-website",
  "version": "0.8.1",
  "scripts": {
    "fmt:check": "npx prettier -c src",
    "fmt:fix": "npx prettier -w src",
    "test": "react-scripts test --env=jsdom",
    "cp-deps": "cp node_modules/bulma/css/*.css public/css/",
    "start": "npm run cp-deps && react-scripts start",
    "build": "npm run cp-deps && react-scripts build",
    "version": "npm run build",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "bulma": "^0.9.3",
    "history": "^4.10.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-url-query": "^1.5.0"
  },
  "devDependencies": {
    "react-scripts": "^4.0.3"
  },
  "author": {
    "name": "Michael Currin",
    "url": "https://MichaelCurrin.github.io/"
  },
  "license": "MIT",
  "homepage": "https://instant-website.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/MichaelCurrin/instant-website.git"
  },
  "keywords": [
    "react",
    "react-url-query",
    "unsplash",
    "query-strings",
    "instant"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
